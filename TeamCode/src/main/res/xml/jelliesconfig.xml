<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<Robot type="FirstInspires-FTC">
    <LynxUsbDevice name="Control Hub Portal" parentModuleAddress="173" serialNumber="(embedded)">
        <LynxModule name="Control Hub" port="173">
            <!-- Motors 5202/3/4 -->
            <goBILDA5202SeriesMotor name="leftBack" port="0" />
            <goBILDA5202SeriesMotor name="rightBack" port="1" />
            <goBILDA5202SeriesMotor name="leftFront" port="2" />
            <goBILDA5202SeriesMotor name="rightFront " port="3" />
            <!-- Servos -->
            <Servo name="flippy" port="0" />
            <Servo name="claw" port="1" />
            <Servo name="twist" port="2" />
            <Servo name="touchy" port="3" />
            <Servo name="rightHang" port="3" />


            <!-- I2C -->
            <LynxEmbeddedIMU name="imu" bus="0" port="0" />
        </LynxModule>
        <LynxModule name="Expansion Hub 2" port="2">
            <!-- Motors -->
            <goBILDA5202SeriesMotor name="slide" port="2" />
            <goBILDA5202SeriesMotor name="flip" port="3" />
            <!-- Servos -->
            <Servo name="leftHang" port="0" />
            <!-- Analog -->
            <REV_VL53L0X_RANGE_SENSOR name="lookyLeft" bus="0" port="0" />
            <REV_VL53L0X_RANGE_SENSOR name="lookyRight" bus="1" port="0" />

        </LynxModule>
    </LynxUsbDevice>
</Robot>

